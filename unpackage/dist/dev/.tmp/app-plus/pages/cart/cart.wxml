<view class="container"><block wx:if="{{!hasLogin}}"><view class="empty"><image src="/static/emptyCart.jpg" mode="aspectFit"></image><view class="empty-tips">空空如也<view data-event-opts="{{[['tap',[['navToLogin',['$event']]]]]}}" class="navigator" bindtap="__e">去登陆></view></view></view></block><block wx:if="{{empty===true}}"><view class="empty"><image src="/static/emptyCart.jpg" mode="aspectFit"></image><view class="empty-tips">空空如也<navigator class="navigator" url="../index/index" open-type="switchTab">随便逛逛></navigator></view></view></block><block wx:if="{{hasLogin}}"><view><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block><view class="{{['cart-item',(index!==cartList.length-1)?'b-b':'']}}"><view class="check-wrapper"><view data-event-opts="{{[['tap',[['check',['item',index]]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox',(item.checked)?'checked':'']}}" bindtap="__e"></view></view><view class="image-wrapper"><image class="{{[item.loaded]}}" src="{{item.image}}" mode="aspectFill" lazy-load data-event-opts="{{[['load',[['onImageLoad',['cartList',index]]]],['error',[['onImageError',['cartList',index]]]]]}}" bindload="__e" binderror="__e"></image></view><view class="item-right"><text class="clamp title">{{item.title}}</text><view class="uni-flex uni-row" style="width:100%;"><view class="flex-item" style="width:80%;"><text class="attr">{{item.attr_val}}</text></view><view class="flex-item del-log" style="width:20%;"><image src="https://pic.youx365.com/del.png" data-event-opts="{{[['tap',[['deleteCartItem',[index]]]]]}}" bindtap="__e" class="_img"></image></view></view><view class="uni-flex uni-row" style="width:100%;"><view class="flex-item" style="width:40%;"><text class="price">{{"¥"+item.price}}</text></view><view class="flex-item" style="width:60%;"><uni-number-box class="step" vue-id="{{'1-'+index}}" min="{{1}}" max="{{item.stock}}" value="{{item.number>item.stock?item.stock:item.number}}" isMax="{{item.number>=item.stock?true:false}}" isMin="{{item.number===1}}" index="{{index}}" data-event-opts="{{[['^eventChange',[['numberChange']]]]}}" bind:eventChange="__e" bind:__l="__l"></uni-number-box></view></view></view></view></block></block></view><view class="guess-like"><view class="header"><image src="https://pic.youx365.com/split-1.png" class="_img"></image><text>猜你喜欢</text><image src="https://pic.youx365.com/split-1.png" class="_img"></image></view><view class="guess-section"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.image}}" mode="aspectFill"></image></view><view class="uni-flex uni-row" style="width:100%;"><view class="flex-item" style="width:50%;"><text class="price">{{"￥"+item.price}}</text></view><view class="flex-item" style="width:50%;text-align:right;"><text class="buysum">2563人已付款</text></view></view><text class="title clamp">{{item.title}}</text></view></block></view></view><view class="action-section"><view class="check-wrapper"><view data-event-opts="{{[['tap',[['check',['all']]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox',(allChecked)?'checked':'']}}" style="font-size:60rpx;height:auto;" bindtap="__e"></view></view><view class="qx">全选</view><view class="total-box"><text class="price">{{"总计：¥"+total}}</text></view><button class="no-border confirm-btn" type="primary" data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" bindtap="__e">去结算</button></view></view></block></view>